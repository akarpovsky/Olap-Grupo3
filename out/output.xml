		<Measure aggregator="sum" name="emision" column="emision"
			datatype="Numeric" />
		<Measure aggregator="sum" name="nube" column="nube" datatype="Integer" />
		<Measure aggregator="avg" name="emisionmala" column="emisionmala"
			datatype="Numeric" />
	</Cube>
