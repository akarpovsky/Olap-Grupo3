				</Level>
			</Hierarchy>
		</Dimension>
		<Measure aggregator="sum" name="nube" datatype="Integer" />
		<Measure aggregator="sum" name="emision" datatype="Numeric" />
		<Measure aggregator="avg" name="emisionmala" datatype="Numeric" />
