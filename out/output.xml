						type="Numeric" />
					<Property name="Ciudad-nombre" column="Ciudad-nombre"
						type="String" />
				</Level>
				<Level name="Pais" uniqueMembers="false" levelType="Regular"
					hideMemberIf="Never" column="Pais-nombre" type="String">
					<Property name="Pais-the_geom" column="Pais-the_geom"
						type="Integer" />
					<Property name="Pais-nombre" column="Pais-nombre" type="String" />
				</Level>
			</Hierarchy>
			<Hierarchy name="Hierarchy" hasAll="true" allMemberName="AllHierarchy"
				primaryKey="Lat">
				<Table name="fabricaFabrica" />
