					hideMemberIf="Never" column="Month-Mes" type="Numeric">
					<Property name="Month-Mes" column="Month-Mes" type="Numeric" />
					<Property name="Month-Nombre" column="Month-Nombre" type="String" />
				</Level>
				<Level name="Year" uniqueMembers="false" levelType="Regular"
					hideMemberIf="Never" column="Year-anio" type="Numeric">
					<Property name="Year-anio" column="Year-anio" type="Numeric" />
					<Property name="Year-esBisiesto" column="Year-esBisiesto"
						type="Numeric" />
				</Level>
			</Hierarchy>
		</Dimension>
		<Dimension name="fabricaFabrica" foreignkKey="Lat">
