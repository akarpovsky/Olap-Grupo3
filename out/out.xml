<?xml version="1.0" encoding="UTF-8"?><Schema name="contaminacion">
    <Cube name="contaminacion" cache="true" enabled="true">
        <Table name="contaminacion_fact"/>
        <Dimension name="inicio_temporal" foreignKey="dia_inicio_temporal">
            <Hierarchy name="calendar" hasAll="true" allMemberName="Allcalendar" primaryKey="dia_inicio_temporal">
                <Table name="inicio_temporal"/>
                <Level name="month" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="month_mes_inicio_temporal" type="Numeric">
                    <Property name="month_nombre" column="month_nombre_inicio_temporal" type="String"/>
                    <Property name="month_mes" column="month_mes_inicio_temporal" type="Numeric"/>
                </Level>
                <Level name="year" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="year_anio_inicio_temporal" type="Numeric">
                    <Property name="year_anio" column="year_anio_inicio_temporal" type="Numeric"/>
                    <Property name="year_esbisiesto" column="year_esbisiesto_inicio_temporal" type="Numeric"/>
                </Level>
            </Hierarchy>
        </Dimension>
        <Dimension name="fin_temporal" foreignKey="dia_fin_temporal">
            <Hierarchy name="calendar" hasAll="true" allMemberName="Allcalendar" primaryKey="dia_fin_temporal">
                <Table name="fin_temporal"/>
                <Level name="month" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="month_mes_fin_temporal" type="Numeric">
                    <Property name="month_nombre" column="month_nombre_fin_temporal" type="String"/>
                    <Property name="month_mes" column="month_mes_fin_temporal" type="Numeric"/>
                </Level>
                <Level name="year" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="year_anio_fin_temporal" type="Numeric">
                    <Property name="year_anio" column="year_anio_fin_temporal" type="Numeric"/>
                    <Property name="year_esbisiesto" column="year_esbisiesto_fin_temporal" type="Numeric"/>
                </Level>
            </Hierarchy>
        </Dimension>
        <Dimension name="fabrica_fabrica" foreignKey="lat_fabrica_fabrica">
            <Hierarchy name="geo location" hasAll="true" allMemberName="Allgeo location" primaryKey="lat_fabrica_fabrica">
                <Table name="fabrica_fabrica"/>
                <Level name="pais" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="pais_nombre_fabrica_fabrica" type="String">
                    <Property name="pais_nombre" column="pais_nombre_fabrica_fabrica" type="String"/>
                    <Property name="pais_the_geom" column="pais_the_geom_fabrica_fabrica" type="Integer"/>
                </Level>
                <Level name="ciudad" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="ciudad_nombre_fabrica_fabrica" type="String">
                    <Property name="ciudad_nombre" column="ciudad_nombre_fabrica_fabrica" type="String"/>
                    <Property name="ciudad_poblacion" column="ciudad_poblacion_fabrica_fabrica" type="Numeric"/>
                </Level>
            </Hierarchy>
            <Hierarchy name="categoria" hasAll="true" allMemberName="Allcategoria" primaryKey="lat_fabrica_fabrica">
                <Table name="fabrica_fabrica"/>
                <Level name="empresa" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="empresa_nombre_fabrica_fabrica" type="String">
                    <Property name="empresa_factanual" column="empresa_factanual_fabrica_fabrica" type="Numeric"/>
                    <Property name="empresa_nombre" column="empresa_nombre_fabrica_fabrica" type="String"/>
                </Level>
                <Level name="rubro" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="rubro_tipoid_fabrica_fabrica" type="Numeric">
                    <Property name="rubro_tipodesc" column="rubro_tipodesc_fabrica_fabrica" type="String"/>
                    <Property name="rubro_tipoid" column="rubro_tipoid_fabrica_fabrica" type="Numeric"/>
                </Level>
            </Hierarchy>
        </Dimension>
        <Dimension name="sustanciadim_sustancia" foreignKey="denominacion_sustanciadim_sustancia">
            <Hierarchy name="denominacion_sustanciadim_sustancia" hasAll="true" allMemberName="Allsustanciadim_sustancia" primaryKey="denominacion_sustanciadim_sustancia">
                <Table name="sustanciadim_sustancia"/>
                <Level name="sustanciadim_sustancia" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" column="denominacion_sustanciadim_sustancia">
                    <Property name="denominacion_sustanciadim_sustancia" column="denominacion_sustanciadim_sustancia" type="String"/>
                </Level>
            </Hierarchy>
        </Dimension>
        <Measure aggregator="avg" name="emisionmala" column="emisionmala" datatype="Numeric"/>
        <Measure aggregator="sum" name="emision" column="emision" datatype="Numeric"/>
        <Measure aggregator="sum" name="nube" column="nube" datatype="Integer"/>
    </Cube>
</Schema>
